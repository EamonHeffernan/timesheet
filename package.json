{
	"name": "timesheet",
	"version": "1.0.0",
	"description": "Timesheet",
	"main": "index.js",
	"dirs": {
		"server": "server",
		"build": "build",
		"next": "app"
	},
	"bin": {
		"setEnv": "server/bin/env/setEnv.mjs"
	},
	"scripts": {
		"build": "tsc && cd %npm_package_dirs_next% && next build",
		"start": "setEnv production node %npm_package_dirs_build%/bin/www.js",
		"test": "setEnv test ts-mocha %npm_package_dirs_server%/**/*.test.ts --reporter spec --exit",
		"dev": "setEnv development nodemon %npm_package_dirs_server%/bin/www",
		"setupTestDB": "setEnv development ts-node %npm_package_dirs_server%/bin/setup/setupTestDB && setEnv production ts-node %npm_package_dirs_server%/bin/setup/setupTestDB",
		"clearDB": "setEnv development ts-node %npm_package_dirs_server%/bin/setup/clearDB && setEnv production ts-node %npm_package_dirs_server%/bin/setup/clearDB"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Torbaz/timesheet.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Torbaz/timesheet/issues"
	},
	"homepage": "https://github.com/Torbaz/timesheet#readme",
	"dependencies": {
		"@typegoose/typegoose": "^7.6.3",
		"bcrypt": "^5.0.1",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"mongoose": "5.10.18",
		"next": "^11.0.1",
		"nodemailer": "^6.6.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"secure-random-string": "^1.1.3",
		"swr": "^0.5.6"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/chai": "^4.2.20",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.12",
		"@types/mocha": "^8.2.3",
		"@types/mongoose": "5.10.2",
		"@types/node": "^12.12.21",
		"@types/nodemailer": "^6.4.4",
		"@types/react": "^17.0.15",
		"@types/react-dom": "^17.0.1",
		"@types/shelljs": "^0.8.9",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"mocha": "^8.0.0",
		"nodemon": "^2.0.9",
		"shelljs": "^0.8.4",
		"ts-mocha": "^8.0.0",
		"ts-node": "^10.1.0",
		"typescript": "^4.3.5"
	}
}
